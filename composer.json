{
    "name": "gfreeau/get-jwt-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle provides a security listener to return a JWT",
    "keywords": ["symfony4", "symfony5", "symfony6", "symfony", "symfony-bundle", "bundle",  "jwt", "jws", "authentication"],
    "homepage": "https://github.com/gfreeau/GfreeauGetJWTBundle",
    "authors": [
        {
            "name": "Greg Freeman",
            "homepage": "http://www.gregfreeman.org"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/http-kernel": "^4.4 || ^5.4 || ^6.0",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0",
        "symfony/security-bundle": "^4.4 ||^5.4 || ^6.0"
    },
    "suggests": {
        "lexik/jwt-authentication-bundle": "^2.0.0"
    },
    "autoload": {
        "psr-0": { "Gfreeau\\Bundle\\GetJWTBundle": "" }
    },
    "target-dir": "Gfreeau/Bundle/GetJWTBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
